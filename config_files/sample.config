<server>
    listen: 0.0.0.0:9995
    server_name: HTML
    error_page: error_pages/
    limit_body_size: 100
    allowed_methods: GET
    <location>
		location: /
        root: HTML/
        allowed_methods: GET, POST
        autoindex: on
        cgi_path: HTML/cgi-bin/
        cgi_ext: .py
    </location>
    <location>
		location: /redirect
        redirect: https://google.com
    </location>
    <location>
		location: /42
        redirect: https://42wolfsburg.de/
    </location>
    <location>
		location: /Hello
        root: HTML/
        allowed_methods: POST, GET
        autoindex: on
        cgi_path: HTML/cgi-bin/
        cgi_ext: .py
    </location>
</server>
<server>
    listen: 0.0.0.0:9996
    server_name: Fileserver
    error_page: error_pages/
    limit_body_size: 1000000
    allowed_methods: GET
    <location>
		location: /
        root: file/
        allowed_methods: POST, GET
        autoindex: off
        cgi_path: HTML/cgi-bin/
        cgi_ext: .py
    </location>
</server>
