<server>
    port: 9998
    host: localhost
    server_name: HTML
    error_page: ./www/errors/
    limit_body_size: 100
    allowed_methods: GET
    <location>
		location: /
        root: ./www/html/
        index: index.html
        redirect: https://google.de/
        allowed_methods: POST, GET
        autoindex: on
    </location>
</server>
