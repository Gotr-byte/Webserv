<server>
    listen: 0.0.0.0:9995
    server_name: HTML
    error_page: ../error_pages/
    limit_body_size: 100
    allowed_methods: GET
    <location>
		location: /
        root: ../HTML/
        allowed_methods: GET, POST
        autoindex: on
    </location>
    <location>
		location: /Hello/
        root: ../HTML/
        index: siege.txt
        allowed_methods: POST, GET
        autoindex: on
    </location>
</server>
<server>
    listen: 0.0.0.0:9996
    server_name: Fileserver
    error_page: ../error_pages/
    limit_body_size: 1000000
    allowed_methods: GET
    <location>
		location: /
        root: ../file/
        index:
        redirect: https://google.de/
        allowed_methods: POST, GET
        autoindex: off
    </location>
</server>
<server>
    listen: 0.0.0.0:9997
    server_name: Siege_test
    error_page: ../error_pages/
    limit_body_size: 100
    allowed_methods: GET
    <location>
		location: /
        index: siege.txt
        root: ../HTML
        allowed_methods: POST, GET
        autoindex: on
    </location>
    <location>
		location: /Hello/
        root: ../HTML/
        index: test.html
        allowed_methods: POST, GET
        autoindex: on
    </location>
</server>
<server>
    listen: 0.0.0.0:9998
    server_name: delete
    error_page: ../error_pages/
    limit_body_size: 100
    allowed_methods: DELETE
    <location>
		location: /
        root: ../file/
        autoindex: on
    </location>
    <location>
		location: /Hello/
        root: ../HTML/
        index: test.html
        allowed_methods: POST, GET
        autoindex: on
    </location>
</server>